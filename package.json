{
  "name": "quest-app-template",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "if [ \"$CF_PAGES\" = \"1\" ] && [ -z \"$NEXT_ON_PAGES\" ]; then npm run pages:build; else next build; fi",
    "start": "next start",
    "lint": "eslint",
    "type-check": "rm -rf .next && rm -f src/components/QuestMap.tsx && tsc --noEmit",
    "compile:compiled": "node --import tsx scripts/compile-compiled-quest.ts",
    "test": "npm run type-check && node --import tsx src/utils/validateQuestData.test.ts && node --import tsx src/utils/puzzleDistribution.test.ts && node --import tsx src/utils/coordinates.test.ts && node --import tsx src/lib/quest-state.test.ts && node --import tsx src/components/object-timeline/timelineUtils.test.ts && node --import tsx src/lib/transcriptionUtils.test.ts",
    "test:e2e": "playwright test",
    "prebuild": "npm test",
    "pages:build": "NEXT_ON_PAGES=1 NPM_CONFIG_LEGACY_PEER_DEPS=true npx @cloudflare/next-on-pages",
    "pages:deploy": "npx wrangler pages deploy .vercel/output/static --project-name=quest-app-template-dev --branch=dev"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.21",
    "aws4fetch": "^1.0.20",
    "fabric": "^6.9.0",
    "gsap": "^3.14.2",
    "itty-router": "^5.0.22",
    "leaflet": "^2.0.0-alpha.1",
    "next": "^16.0.8",
    "opensheetmusicdisplay": "^1.9.3",
    "pixi.js": "^8.14.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tone": "^15.1.22",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  }
}
